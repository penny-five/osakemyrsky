  "author": "penny-five",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "16"
  },
  "dependencies": {
    "@apollo/client": "3.4.16",
    "@heroicons/react": "1.0.4",
    "classnames": "2.3.1",
    "date-fns": "2.25.0",
    "graphql": "15.6.1",
    "jwt-decode": "3.1.2",
    "next": "11.1.2",
    "next-auth": "4.0.0-beta.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@tailwindcss/forms": "0.3.4",
    "@tsconfig/next": "1.0.0",
    "@types/react": "17.0.30",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "audit-ci": "5.1.2",
    "autoprefixer": "10.3.7",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "8.3.0",
    "lint-staged": "11.2.3",
    "postcss": "8.3.9",
    "prettier": "2.4.1",
    "tailwindcss": "2.2.17",
    "typescript": "4.4.4"
  },
  "scripts": {
    "precommit": "lint-staged",
    "audit-ci": "audit-ci --high --report-type=full",
    "prettify": "prettier --write .",
    "lint": "NEXT_TELEMETRY_DISABLED=1 next lint",
    "dev": "NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--inspect' next dev -p 8080",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "NEXT_TELEMETRY_DISABLED=1 next start"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint",
      "prettier --write"
    ],
    "**/*": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "last 2 chrome versions"
  ]
}
