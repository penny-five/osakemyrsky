    "passport-jwt": "4.0.0",
    "pg": "8.7.1",
    "pino-http": "5.8.0",
    "pino-pretty": "7.1.0",
    "promise-retry": "2.0.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.4.0",
    "tough-cookie": "4.0.0"
