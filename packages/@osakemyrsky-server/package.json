    "google-auth-library": "7.10.2",
    "got-scraping": "3.2.6",
    "graphql": "15.6.1",
    "jsonwebtoken": "8.5.1",
    "jwk-to-pem": "2.0.5",
    "knex": "0.21.21",
    "nestjs-pino": "2.2.0",
