  "private": true,
  "engines": {
    "node": "16"
  },
  "dependencies": {
    "@nestjs/common": "8.1.2",
    "@nestjs/config": "1.1.0",
    "@nestjs/core": "8.1.2",
    "@nestjs/graphql": "9.1.1",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.0.1",
    "@nestjs/platform-express": "8.1.2",
    "@willsoto/nestjs-objection": "5.1.0",
